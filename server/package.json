{
  "name": "vrock-claudia",
  "version": "1.0.0",
  "description": "Forge Viewer with Faster Serverless deployment using Claudia.js",
  "main": "vrock.js",
  "scripts": {
    "create": "claudia create --handler vrock.handler --deploy-proxy-api --region us-west-2 --version vrock",
    "setvars": "claudia set-version --version vrock --set-env FORGE_CLIENT_ID=4zozNC5sPLS,FORGE_CLIENT_SECRET=xxxsC6Y,FORGE_URN=xxxxxxcnZ0",
    "update": "claudia update --version vrock",
    "destroy": "claudia destroy"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "aws-sdk": "^2.141.0",
    "aws-serverless-express": "^1.3.0",
    "express": "^4.14.0",
    "forge-apis": "^0.3.0"
  },
  "devDependencies": {
    "claudia": "^2.1.0"
  }
}
